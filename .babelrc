{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    ],
    "@babel/plugin-transform-export-namespace-from"
  ],
  "env": {
    "production": {
      "plugins": ["transform-remove-console"]
    }
  }
}
